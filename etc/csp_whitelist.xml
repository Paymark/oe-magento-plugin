<?xml version="1.0" encoding="UTF-8"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
               xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="paymark_script" type="host">*.paymark.co.nz</value>
            </values>
        </policy>
        <policy id="frame-src">
            <values>
                <value id="paymark_frame" type="host">*.paymark.co.nz</value>
            </values>
        </policy>
        <policy id="default-src">
            <values>
                <value id="paymark_fallback" type="host">*.paymark.co.nz</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
